{
    "name": "@dataunions/client",
    "version": "3.0.0-alpha.2",
    "description": "JavaScript client library for DataUnions",
    "private": false,
    "repository": {
        "type": "git",
        "url": "git+https://github.com/dataunions/data-union.git",
        "directory": "packages/client"
    },
    "types": "./dist/types/src/index.d.ts",
    "main": "./dist/src/index.js",
    "browser": "./dist/web/index.js",
    "exports": {
        "default": {
            "import": "./dist/src/index-esm.mjs",
            "require": "./dist/src/index-commonjs.js"
        }
    },
    "scripts": {
        "bootstrap-dist": "npm run vendor && npm run copy-package && npm run fix-esm",
        "build": "npm run bootstrap-dist && npm run build-node",
        "build-browser": "npm run build-browser-iife -- --outdir=dist/web && npm run build-browser-esm -- --outdir=dist/web",
        "build-browser-base": "esbuild src/index.ts --bundle --sourcemap --target=chrome101 --tsconfig=tsconfig.browser.json --legal-comments=linked --define:process.env.NODE_ENV=\\\"${NODE_ENV:-development}\\\" --inject:./process-shim.js",
        "build-browser-esm": "npm run build-browser-base --outfile=dist/web/index.esm.js -- --format=esm",
        "build-browser-iife": "npm run build-browser-base -- --format=iife --global-name=DataUnions",
        "build-node": "npm run bootstrap-dist && tsc --build ./tsconfig.node.json",
        "build-production": "npm run clean; npm run bootstrap-dist && NODE_ENV=production npm run build && NODE_ENV=production npm run build-browser",
        "check": "tsc -p ./tsconfig.json --noEmit",
        "clean": "jest --clearCache || true; rm -rf dist vendor *.tsbuildinfo node_modules/.cache || true",
        "copy-package": "mkdir -p dist/; node copy-package.js; cp -f README.md LICENSE readme-header-img.png dist; true",
        "fix-esm": "mkdir -p dist/src/; cp -Rf src/index-esm.mjs dist/src/index-esm.mjs",
        "lint": "eslint --cache --cache-location=node_modules/.cache/.eslintcache/ '*/**/*.{js,ts}'",
        "postbuild-node": "npm run copy-package",
        "start-browser": "npm run build-browser-iife -- --servedir=test/browser --outdir=test/browser",
        "test": "npm run test-exports",
        "test-exports": "cd test/exports && npm run link && tsc --noEmit --project ./tsconfig.json && npm test",
        "integration-test": "jest --forceExit --maxWorkers=1 test/end-to-end",
        "vendor": "SRCDIR=`node -p \"path.dirname(require.resolve('quick-lru'))\"` bash -c 'mkdir -p vendor/quick-lru; cp -n $SRCDIR/index.d.ts vendor/quick-lru; cp -n $SRCDIR/index.js vendor/quick-lru'; true"
    },
    "engines": {
        "node": ">= 16"
    },
    "author": "Streamr",
    "license": "Apache-2.0",
    "devDependencies": {
        "@dataunions/dev-config": "3.0.0-alpha.2",
        "@dataunions/eslint-config": "3.0.0-alpha.2",
        "@ethersproject/wordlists": "^5.5.0",
        "@types/cors": "^2.8.12",
        "@types/debug": "^4.1.7",
        "@types/express": "^4.17.13",
        "@types/jest": "28.1.3",
        "@types/lodash": "^4.14.175",
        "@types/node": "^16.10.6",
        "@types/node-fetch": "^2.5.12",
        "@types/uuid": "^8.3.1",
        "@typescript-eslint/eslint-plugin": "^5.0.0",
        "@typescript-eslint/parser": "^5.0.0",
        "benchmark": "^2.1.4",
        "buffer": "^6.0.3",
        "bytes": "^3.1.1",
        "chromedriver": "^100.0.0",
        "concurrently": "^6.4.0",
        "cors": "^2.8.5",
        "crypto-browserify": "^3.12.0",
        "esbuild": "^0.14.38",
        "eslint": "^7.32.0",
        "eslint-config-airbnb": "^18.2.1",
        "eslint-loader": "^4.0.2",
        "eslint-plugin-import": "^2.25.3",
        "eslint-plugin-promise": "^5.1.0",
        "ethers": "^5.4.7",
        "express": "^4.17.1",
        "git-revision-webpack-plugin": "5.0.0",
        "jest": "28.1.2",
        "jest-circus": "28.1.2",
        "jest-extended": "^0.11.5",
        "jest-leak-detector": "28.1.1",
        "nightwatch": "^1.7.12",
        "pretty-bytes": "^5.6.0",
        "process": "^0.11.10",
        "streamr-test-utils": "^2.0.0",
        "ts-jest": "28.0.5",
        "ts-loader": "^9.2.6",
        "typescript": "4.7.4",
        "util": "^0.12.4",
        "weak-napi": "^2.0.2"
    },
    "dependencies": {
        "@dataunions/contracts": "3.0.0-alpha.2",
        "@ethersproject/abi": "^5.5.0",
        "@ethersproject/abstract-signer": "^5.5.0",
        "@ethersproject/address": "^5.5.0",
        "@ethersproject/bignumber": "^5.5.0",
        "@ethersproject/bytes": "^5.5.0",
        "@ethersproject/constants": "^5.5.0",
        "@ethersproject/contracts": "^5.5.0",
        "@ethersproject/keccak256": "^5.5.0",
        "@ethersproject/providers": "^5.5.0",
        "@ethersproject/transactions": "^5.5.0",
        "@ethersproject/units": "^5.5.0",
        "@ethersproject/wallet": "^5.5.0",
        "@ethersproject/web": "^5.5.0",
        "debug": "^4.3.2",
        "eventemitter3": "^4.0.7",
        "lodash": "^4.17.21",
        "mem": "^8.1.1",
        "node-fetch": "^2.6.6",
        "p-limit": "^3.1.0",
        "p-memoize": "4.0.3",
        "quick-lru": "^6.0.1",
        "readable-stream": "^3.6.0",
        "reflect-metadata": "^0.1.13",
        "ts-toolbelt": "^9.6.0",
        "tsyringe": "^4.6.0",
        "uuid": "^8.3.2"
    },
    "optionalDependencies": {
        "bufferutil": "^4.0.5",
        "utf-8-validate": "^5.0.7"
    },
    "bugs": {
        "url": "https://github.com/dataunions/data-union/issues"
    },
    "homepage": "https://github.com/dataunions/data-union#readme",
    "publishConfig": {
        "access": "public"
    }
}
